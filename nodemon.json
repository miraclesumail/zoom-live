{
    "restartable": "rs",
    "ignore": [
        ".git",
        ".svn",
        "node_modules/**/node_modules"
    ],
    "verbose": true,
    "watch": [
        "index.ts"
    ],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "ts json",
    "exec": "ts-node -r tsconfig-paths/register ./index.ts"
}